<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>log4j-init</servlet-name>
        <servlet-class>cl.cnsv.wsreporteproyeccion.servlet.Log4jInitServlet</servlet-class>
        <init-param>
            <param-name>log4j-init-file</param-name>
            <param-value>log4j.properties</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>WS_ReporteProyeccion</servlet-name>
        <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
        <init-param>
            <param-name>com.sun.jersey.config.property.packages</param-name>
            <param-value>cl.cnsv.wsreporteproyeccion.ws</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ReporteProyeccionServlet</servlet-name>
        <servlet-class>cl.cnsv.wsreporteproyeccion.servlet.ReporteProyeccionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReporteProyeccionServlet</servlet-name>
        <url-pattern>/ReporteProyeccionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
